package com.pojo;

public class Person {
	
	private int age;
	private String name;
	
	final public void display() {
		System.out.println("name:-"+name+"\t"+"age:-"+age);
	}
public Person() {
	age=22;
	name="Rashmi";
}

public Person(int age, String name) {	
	this.age=age;
	this.name=name;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getAge() {
	return age;
}
public void setAge(int a) {
	age=a;
}
}
